{
  "name": "backlog-omnibox-search",
  "version": "0.0.1",
  "description": "Search backlog issues by your omnibox.",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "parcel watch src/background.ts src/options/options.html --public-url=/dist",
    "build": "npm run clean && npm ci && parcel build src/background.ts src/options/options.html --no-source-maps --no-cache",
    "clean": "rm -rf node_modules .cache dist"
  },
  "author": "mmktomato",
  "license": "MIT",
  "devDependencies": {
    "@types/firefox-webext-browser": "^70.0.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-web-ext-tool": "^0.1.2",
    "typescript": "^3.8.3",
    "web-ext": "^4.1.0",
    "web-ext-types": "^3.2.1"
  },
  "dependencies": {
    "lit-element": "^2.3.1",
    "ts-debounce": "^2.0.1",
    "webextension-polyfill": "^0.6.0"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "webExt": {
    "run": {
      "target": [
        "chromium",
        "firefox-desktop"
      ],
      "startUrl": [
        "chrome://extensions",
        "about:debugging#/runtime/this-firefox"
      ]
    }
  }
}
